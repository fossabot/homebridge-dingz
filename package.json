{
  "displayName": "Homebridge Dingz Plugin",
  "name": "homebridge-dingz",
  "version": "1.0.1",
  "author": "johannrichard",
  "description": "Homebridge Plugin for Dingz & myStrom Devices. Implements some (but not all, due to API limitations) functions of a [dingz](https://dingz.ch) Smart Home Device.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/johannrichard/homebridge-dingz.git"
  },
  "bugs": {
    "url": "https://github.com/johannrichard/homebridge-dingz/issues"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">0.4.53"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "homebridge-plugin",
    "dingz",
    "smarthome",
    "iot"
  ],
  "dependencies": {
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "abort-controller": "^3.0.0",
    "async-mutex": "^0.2.2",
    "cockatiel": "^0.1.5",
    "form-data": "^3.0.0",
    "form-urlencoded": "^4.1.3",
    "node-fetch": "^2.6.0",
    "popsicle": "^12.0.5",
    "simple-color-converter": "^2.1.5"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "homebridge": "^1.0.4",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
